<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.mapper.MgroupMapper">
	<select id="getOneGroup" resultType="kr.co.domain.MgroupVO">
		select * from mgroup
		where m_id = #{m_id} and g_name = #{g_name}
	</select>

</mapper>